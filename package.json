{
  "name": "targetprocess-mcp",
  "version": "0.9.0",
  "description": "A tool server for Apptio Target Process",
  "author": "Aaron Bockelie <aaronsb@gmail.com> (https://github.com/aaronsb)",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronsb/apptio-target-process-mcp.git"
  },
  "license": "MIT",
  "keywords": [
    "targetprocess",
    "mcp",
    "model-context-protocol",
    "ai-assistant",
    "claude",
    "agile",
    "project-management"
  ],
  "private": false,
  "type": "module",  "bin": {
    "targetprocess-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "[ \"$NODE_ENV\" != \"production\" ] && npm run build || exit 0",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "lint": "rm -rf build && eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.4"
  },  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.3.3"
  }
}
